		"@sveltejs/kit": "^1.27.7",
		"@typescript-eslint/eslint-plugin": "^6.13.2",
		"@typescript-eslint/parser": "^6.13.2",
		"date-fns": "^2.30.0",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "5.0.0-next.22",
		"svelte-check": "^3.6.2",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.7",
		"vitest": "^1.0.2"
